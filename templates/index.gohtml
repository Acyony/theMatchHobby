    <form method="post" action="/new-user" enctype="multipart/form-data">
        <div class="input-group mb-3">
            <input type="file" class="form-control" id="inputGroupFile02" name="Avatar" required>
            <label class="input-group-text" for="inputGroupFile02">Upload</label>
        </div>

        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Name</span>
            <input type="text" class="form-control" name="Name" aria-describedby="inputGroup-sizing-sm" value="aaaa">
        </div>

        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Age</span>
            <input type="number" class="form-control" name="Age" aria-describedby="inputGroup-sizing-sm"  value="21">
        </div>

        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Email</span>
            <input type="email" class="form-control" name="Email" aria-describedby="inputGroup-sizing-sm"  value="aaaa@aaa.com">
        </div>

        <div class="input-group input-group-sm mb-3">
            <span class="input-group-text" id="inputGroup-sizing-sm">Hobbies</span>
            <input type="text" class="form-control" name="Hobbies" aria-describedby="inputGroup-sizing-sm" value="aaaa">
        </div>
        <div>
            <input type="submit" value="Submit" class="btn btn-outline-secondary">
        </div>
    </form>
